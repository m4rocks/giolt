{
	"$schema": "https://unpkg.com/wrangler@4.32.0/config-schema.json",
	"name": "giolt",
	"main": "./dist/_worker.js/index.js",
	"compatibility_date": "2025-08-25",
	"compatibility_flags": ["nodejs_compat"],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"observability": {
		"enabled": true,
		"logs": {
			"enabled": true,
			"invocation_logs": true
		}
	},
	"d1_databases": [{
		"database_id": "64a8e763-42e9-4146-83b9-ca48cb48aa70",
		"binding": "DB",
		"database_name": "giolt-db",
		"migrations_dir": "./drizzle"
	}]
}
