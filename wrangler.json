{
	"$schema": "https://unpkg.com/wrangler@4.32.0/config-schema.json",
	"name": "giolt",
	"main": "./dist/_worker.js/index.js",
	"compatibility_date": "2025-08-25",
	"compatibility_flags": ["nodejs_compat"],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"d1_databases": [{
		"database_id": "giolt-db",
		"binding": "DB",
		"database_name": "giolt-db",
		"migrations_dir": "./drizzle"
	}]
}
